{
    "name": "phergie/phergie-irc-plugin-react-commandalias",
    "description": "Phergie plugin for enabling the use of custom aliases for existing bot commands",
    "keywords": [ "irc", "bot", "react", "plugin" ],
    "license": "BSD-2-Clause",
    "support": {
        "email": "team@phergie.org",
        "irc": "irc://irc.freenode.net/phergie",
        "issues": "https://github.com/phergie/phergie-irc-plugin-react-commandalias/issues",
        "source": "https://github.com/phergie/phergie-irc-plugin-react-commandalias"
    },
    "minimum-stability": "dev",
    "require": {
        "phergie/phergie-irc-bot-react": "dev-master",
        "phergie/phergie-irc-plugin-react-command": "dev-master"

    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*",
        "phake/phake": "2.0.0-beta2"
    },
    "suggest": {
        "phergie/phergie-irc-plugin-react-commandhelp": "provides interactive help for commands"
    },
    "autoload": {
        "psr-4": {
            "Phergie\\Irc\\Plugin\\React\\CommandAlias\\": "src"
        }
    }
}
